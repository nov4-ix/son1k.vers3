# Son1kVers3 Environment Configuration

# Suno AI Integration
SUNO_SESSION_ID=your_suno_session_id_here
SUNO_COOKIE=your_suno_cookie_here
SUNO_BASE_URL=https://studio-api.suno.ai
SUNO_TIMEOUT=120
SUNO_MAX_RETRIES=3
SUNO_ENABLE=true

# Security
API_SECRET_KEY=your-secret-key-here
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8000"]
MAX_FILE_SIZE_MB=100
RATE_LIMIT_PER_MINUTE=60

# Database (if needed)
DATABASE_URL=sqlite:///./son1k.db

# Audio Processing
MAX_DURATION_FREE=8
MAX_DURATION_PRO=30
MAX_DURATION_BETA=60
SAMPLE_RATE=32000

# AI Models
MUSICGEN_MODEL=facebook/musicgen-medium
APLIO_MODELS_PATH=./models/aplio
ENABLE_GPU=false

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/son1k.log

# Development
DEBUG=false
RELOAD=true